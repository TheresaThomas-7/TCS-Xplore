//To find the reverse of a user inputted number
import java.util.Scanner;
public class Solution
{
    public static void main(String[] args)
    {
        int num,rem,rev=0;
        Scanner sc=new Scanner(System.in);
        num=sc.nextInt();
        while(num!=0)
        {
            rem=num%10;
            rev=rev*10+rem;
            num=num/10;
        }
        System.out.println(rev);
    }
}
